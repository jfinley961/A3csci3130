{
  "rules": {
    ".read": "true",
    ".write": "true",
    "Businesses": {
      "$business": {
        "number": {
          ".validate": "newData.val().matches(/[0-9][0-9][0-9][0-9][0-9][0-9][0-9][0-9][0-9]/)"
        },
        "name": {
          ".validate": "newData.isString() && newData.val().length > 1 && newData.val().length < 49"
        },
        "business": {
          ".validate": "newData.isString() && newData.val().matches(/(Fisher)|(Distributor)|(Processor)|(Fish Monger)/)"
        },
        "address": {
          ".validate": "newData.isString() && newData.val().length < 50"
        },
        "province": {
          ".validate": "(newData.isString() && newData.val().matches(/(AB)|(BC)|(MB)|(NB)|(NL)|(NS)|(NT)|(NU)|(ON)|(PE)|(QC)|(SK)|(YT)/)) || newData.val().length == 0"
        },
        "uid": {
          ".validate": "newData.isString() && newData.val().length > 1"
        }
      }
    }
  }
}